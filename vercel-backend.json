{
  "version": 2,
  "builds": [
    {
      "src": "backend/agency_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/agency_backend/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "backend/agency_backend/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "agency_backend.settings"
  }
}